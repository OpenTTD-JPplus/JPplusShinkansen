        1:  sw_e3_series_2nd_car_reverse_check;
        sw_e3_series_toreiyu_2nd_car_reverse_check;				
      }

    // 6car (R Set)
    switch (FEAT_TRAINS, SELF, sw_e3_series_6car_2nd_not_reversed, position_in_consist) {				
        1:  sw_e3_series_clog2f;     
        sw_e3_series_pantof;			
