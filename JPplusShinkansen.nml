# 1 "src/header.pnml" 1
grf {
	grfid: 					"KR03";
	name: 					string(STR_GRF_NAME);
	desc:					string(STR_GRF_DESC);
	version:				62;	// v0.6.2
	min_compatible_version: 60;	// v0.6.0
		// Hide logo
		param 3 {
        	param_hide_logo {
				type: bool;
				name: string(STR_PARAM_HIDE_LOGO);
				desc: string(STR_PARAM_HIDE_LOGO);
				bit: 0;
				def_value: 1;
            }
        }
		
		//japanset3 removal
		param 4 {
			 param_japansetdisable {
					name: string(STR_PARAM_NAME_JAPANSETDISABLE);
					desc: string(STR_PARAM_DESC_JAPANSETDISABLE);
					type: bool;
					bit:  0;
					def_value: 0;
			 }
		}
		//pax capacity
		param 2 {
				param_basecapacity {
						type: int;
